<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageButton
        android:id="@+id/backButton"
        android:layout_width="@dimen/_62sdp"
        android:layout_height="@dimen/_64sdp"
        android:layout_marginStart="@dimen/_10sdp"
        android:layout_marginTop="@dimen/_8sdp"
        android:layout_marginBottom="@dimen/_20sdp"
        android:background="#00000000"
        android:contentDescription="Back Button"
        android:src="@drawable/baseline_arrow_back_ios_new_25"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/categoriesText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_1sdp"
        android:layout_marginTop="@dimen/_18sdp"
        android:layout_marginEnd="@dimen/_16sdp"
        android:layout_marginBottom="@dimen/_16sdp"
        android:fontFamily="@font/poppinsbold"
        android:text="Casual"
        android:textColor="@color/black"
        android:textSize="@dimen/_19ssp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@id/backButton"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.013" />

    <ImageButton
        android:id="@+id/newbalance_574_Btn"
        android:layout_width="@dimen/_125sdp"
        android:layout_height="@dimen/_111sdp"
        android:layout_marginStart="@dimen/_20sdp"
        android:layout_marginBottom="@dimen/_399sdp"
        android:background="@drawable/btn_rounded"
        android:contentDescription="basketball_1"
        android:paddingRight="@dimen/_30sdp"
        android:paddingBottom="@dimen/_25sdp"
        android:src="@drawable/newbalance_574"
        android:paddingTop="@dimen/_40sdp"
        android:paddingLeft="@dimen/_20sdp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintDimensionRatio="H,250:125.4"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/newbalance_574_Text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Balance 574"
        android:textSize="@dimen/_14sdp"
        android:fontFamily="@font/helveticanowtextbold"
        android:textColor="@color/black"
        app:layout_constraintTop_toBottomOf="@id/newbalance_574_Btn"
        app:layout_constraintStart_toStartOf="@id/newbalance_574_Btn"
        app:layout_constraintEnd_toEndOf="@id/newbalance_574_Btn"
        android:layout_marginTop="@dimen/_8sdp"
        android:layout_marginBottom="@dimen/_8sdp" />

    <!-- Second ImageButton and TextView -->

    <ImageButton
        android:id="@+id/converse_chuck_Btn"
        android:layout_width="@dimen/_125sdp"
        android:layout_height="@dimen/_111sdp"
        android:layout_marginStart="@dimen/_15sdp"
        android:layout_marginBottom="@dimen/_1sdp"
        android:background="@drawable/btn_rounded"
        android:contentDescription="another_button"
        android:src="@drawable/converse_chuck"
        android:paddingTop="@dimen/_20sdp"
        android:paddingRight="@dimen/_5sdp"
        app:layout_constraintBottom_toBottomOf="@id/newbalance_574_Btn"
        app:layout_constraintDimensionRatio="H,250:125.4"
        app:layout_constraintStart_toEndOf="@id/newbalance_574_Btn" />

    <TextView
        android:id="@+id/converse_chuck_Text"
        android:layout_width="@dimen/_120sdp"
        android:layout_height="@dimen/_32sdp"
        android:layout_marginTop="@dimen/_8sdp"
        android:fontFamily="@font/helveticanowtextbold"
        android:text="Converse Chuck Taylor All-Star"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="@dimen/_14sdp"
        app:layout_constraintEnd_toEndOf="@id/converse_chuck_Btn"
        app:layout_constraintStart_toStartOf="@id/converse_chuck_Btn"
        app:layout_constraintTop_toBottomOf="@id/converse_chuck_Btn" />

    <ImageButton
        android:id="@+id/onitsuka_mexico_Btn"
        android:layout_width="@dimen/_125sdp"
        android:layout_height="@dimen/_111sdp"
        android:layout_marginStart="@dimen/_20sdp"
        android:layout_marginTop="@dimen/_24sdp"
        android:background="@drawable/btn_rounded"
        android:contentDescription="third_button"
        android:paddingTop="@dimen/_10sdp"
        android:paddingRight="@dimen/_5sdp"
        android:paddingBottom="@dimen/_40sdp"
        android:src="@drawable/onitsuka_mexico"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/newbalance_574_Text" />

    <TextView
        android:id="@+id/onitsuka_mexico_Text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Onitsuka Tiger"
        android:textSize="@dimen/_14ssp"
        android:fontFamily="@font/helveticanowtextbold"
        android:textColor="@color/black"
        app:layout_constraintTop_toBottomOf="@id/onitsuka_mexico_Btn"
        app:layout_constraintStart_toStartOf="@id/onitsuka_mexico_Btn"
        app:layout_constraintEnd_toEndOf="@id/onitsuka_mexico_Btn"
        android:layout_marginTop="@dimen/_8sdp"
        android:layout_marginBottom="@dimen/_8sdp" />
    <!-- Fourth ImageButton and TextView -->

    <ImageButton
        android:id="@+id/vans_slip_Btn"
        android:layout_width="@dimen/_125sdp"
        android:layout_height="@dimen/_111sdp"
        android:layout_marginStart="@dimen/_30sdp"
        android:layout_marginTop="@dimen/_15sdp"
        android:background="@drawable/btn_rounded"
        android:contentDescription="fourth_button"
        android:paddingRight="@dimen/_25sdp"
        android:paddingBottom="@dimen/_40sdp"
        android:src="@drawable/vans_slip"
        app:layout_constraintStart_toEndOf="@id/onitsuka_mexico_Text"
        app:layout_constraintTop_toBottomOf="@id/converse_chuck_Text" />

    <ImageButton
        android:id="@+id/adidasbrandsuperstar_Btn"
        android:layout_width="@dimen/_125sdp"
        android:layout_height="@dimen/_111sdp"
        android:layout_marginStart="@dimen/_140sdp"
        android:layout_marginTop="@dimen/_20sdp"
        android:background="@drawable/btn_rounded"
        android:contentDescription="basketball_1"
        android:paddingLeft="@dimen/_10sdp"
        android:paddingTop="@dimen/_10sdp"
        android:paddingRight="@dimen/_30sdp"
        android:paddingBottom="@dimen/_25sdp"
        android:src="@drawable/adida_superstar"
        app:layout_constraintStart_toStartOf="@id/nike_air_Btn"
        app:layout_constraintTop_toBottomOf="@id/vans_slip_Text"
        app:layout_constraintStart_toEndOf="@id/nike_air_Btn"/>

    <TextView
        android:id="@+id/adidasbrandsuperstar_Text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Adidas Superstar"
        android:textSize="@dimen/_14ssp"
        android:fontFamily="@font/helveticanowtextbold"
        android:textColor="@color/black"
        app:layout_constraintTop_toBottomOf="@id/adidasbrandsuperstar_Btn"
        app:layout_constraintStart_toStartOf="@id/adidasbrandsuperstar_Btn"
        app:layout_constraintEnd_toEndOf="@id/adidasbrandsuperstar_Btn"
        android:layout_marginTop="@dimen/_8sdp"
        android:layout_marginBottom="@dimen/_8sdp" />

    <TextView
        android:id="@+id/vans_slip_Text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Vans Slip-On"
        android:textSize="@dimen/_14ssp"
        android:fontFamily="@font/helveticanowtextbold"
        android:textColor="@color/black"
        app:layout_constraintTop_toBottomOf="@id/vans_slip_Btn"
        app:layout_constraintStart_toStartOf="@id/vans_slip_Btn"
        app:layout_constraintEnd_toEndOf="@id/vans_slip_Btn"
        android:layout_marginTop="@dimen/_8sdp"
        android:layout_marginBottom="@dimen/_8sdp" />

    <!-- Fifth ImageButton and TextView -->

    <ImageButton
        android:id="@+id/nike_air_Btn"
        android:layout_width="@dimen/_125sdp"
        android:layout_height="@dimen/_111sdp"
        android:layout_marginStart="@dimen/_20sdp"
        android:layout_marginTop="@dimen/_20sdp"
        android:background="@drawable/btn_rounded"
        android:contentDescription="fifth_button"
        android:paddingTop="@dimen/_15sdp"
        android:paddingRight="@dimen/_15sdp"
        android:paddingBottom="@dimen/_40sdp"
        android:src="@drawable/nike_air"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/onitsuka_mexico_Text" />

    <TextView
        android:id="@+id/nike_air_Text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nike Air Force 1"
        android:textSize="@dimen/_14ssp"
        android:fontFamily="@font/helveticanowtextbold"
        android:textColor="@color/black"
        app:layout_constraintTop_toBottomOf="@id/nike_air_Btn"
        app:layout_constraintStart_toStartOf="@id/nike_air_Btn"
        app:layout_constraintEnd_toEndOf="@id/nike_air_Btn"
        android:layout_marginTop="@dimen/_8sdp"
        android:layout_marginBottom="@dimen/_8sdp" />

</androidx.constraintlayout.widget.ConstraintLayout>

